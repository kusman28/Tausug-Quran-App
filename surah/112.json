{
    "112": {
        "number": "112",
        "name": "الاخلاص",
        "name_latin": "Al-Ikhlas",
        "number_of_ayah": "4",
        "text": {
            "1": "قُلْ هُوَ اللّٰهُ اَحَدٌۚ",
            "2": "اَللّٰهُ الصَّمَدُۚ",
            "3": "لَمْ يَلِدْ وَلَمْ يُوْلَدْۙ",
            "4": "وَلَمْ يَكُنْ لَّهٗ كُفُوًا اَحَدٌ ࣖ"
        },
        "translations": {
            "id": {
                "name": "Ikhlas",
                "text": {
                    "1": "Pamungan: Siya in Allah, Tuhan Tunggal.",
                    "2": "In Allah, As-Samad (Tuhan Kakkal Hahajatun).",
                    "3": "Wala Siya imanak iban wala iyanak.",
                    "4": "Iban wayruun sibu agiad Niya hangsulag unu-unu."
                }
            }
        }
    }
}