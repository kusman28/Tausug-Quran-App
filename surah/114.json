{
    "114": {
        "number": "114",
        "name": "الناس",
        "name_latin": "An-Nas",
        "number_of_ayah": "6",
        "text": {
            "1": "قُلْ اَعُوْذُ بِرَبِّ النَّاسِۙ",
            "2": "مَلِكِ النَّاسِۙ",
            "3": "اِلٰهِ النَّاسِۙ",
            "4": "مِنْ شَرِّ الْوَسْوَاسِ ەۙ الْخَنَّاسِۖ",
            "5": "الَّذِيْ يُوَسْوِسُ فِيْ صُدُوْرِ النَّاسِۙ",
            "6": "مِنَ الْجِنَّةِ وَالنَّاسِ ࣖ "
        },
        "translations": {
            "id": {
                "name": "Manusiya",
                "text": {
                    "1": "Pamungan: Magpatilibun aku pa Tuhan sin Manusiya",
                    "2": "Tagmilik ha Manusiya.",
                    "3": "Tuhan pagtag-ipunan sin Manusiya.",
                    "4": "Daying ha kangian sin panghigung-hagas sin Saytan Al-Khannas.",
                    "5": "Amuin manghigung-hagas pa daghal (Atay) sin Manusiya.",
                    "6": "Daying na mayan ha pihak Jinn iban Manusiya."
                }
            }
        }
    }
}